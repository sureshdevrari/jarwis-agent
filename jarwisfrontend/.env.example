# Jarwis Frontend Environment Configuration
# Copy this file to .env.local for local development

# ============== FastAPI Backend (Required) ==============
# API Base URL - Your FastAPI backend server
REACT_APP_API_URL=http://localhost:8000

# ============== Application Settings ==============
REACT_APP_NAME=Jarwis AGI Pen Test
REACT_APP_VERSION=1.0.0

# ============== Feature Flags ==============
REACT_APP_ENABLE_SOCIAL_LOGIN=false
REACT_APP_ENABLE_MOBILE_SCAN=false
REACT_APP_ENABLE_CLOUD_SCAN=false

# ============== Server Configuration ==============
# Override server URL for agent connections (optional)
# If not set, auto-detected from API or window.location.origin
# VITE_JARWIS_SERVER_URL=https://app.jarwis.ai

# ============== reCAPTCHA (Optional) ==============
REACT_APP_CAPTCHA_SITE_KEY=6LdKAT8sAAAAANBHEEcAF6n8NtNGCoWsubRMYfB7

